<div ng-app="AppAddressBookModule">
    <div ng-controller="AddressBookController">
        <h3>Add Profile</h3>
        <form name="addAddressBook" >

            <div>Name: <input name="name" ng-model="AddressBook.Name" /></div>
            <div>First Name: <input name="FirstName" ng-model="AddressBook.FirstName" /></div>
            <div>Last Name:<input name="LastName" ng-model="AddressBook.LastName" /></div>
            <div>Company:<input name="Company" ng-model="AddressBook.Company" /></div>
            <div>Cell Phone:<input name="CellPhone" ng-model="AddressBook.CellPhone" /></div>
            <div>Fax:<input name="Fax" ng-model="AddressBook.Fax" /></div>
            <div>Shipping Address:<input name="BillingAddress" ng-model="AddressBook.BillingAddress" /></div>
            <div>Shipping City:<input name="BillingCity" ng-model="AddressBook.BillingCity" /></div>
            <div>Shipping State:<input name="BillingState" ng-model="AddressBook.BillingState" /></div>
            <div>Shipping Zipcode:<input name="BillingZipCode" ng-model="AddressBook.BillingZipCode" /></div>
            <div>Product Key:<input name="ProductKey" ng-model="AddressBook.ProductKey" /></div>

            <div>
                Type:
                <select ng-model="AddressBook.PeopleXRefId">
                    <option value="customer" selected>Customer</option>
                    <option value="employee">Employee</option>
                    <option value="vendor">Vendor</option>
                </select>
            </div>

           
            <div><button ng-click="addPerson()">Submit</button></div>
           

             {{Message}}

        </form>
        </div>
    </div>
